<h1>Listing cheats</h1>

<table>
  <tr>
    <th>Language</th>
    <th>Content</th>
  </tr>

<% @cheats.each do |cheat| %>
  <tr>
    <td><%=h cheat.language_id %></td>
    <td><%=h cheat.content %></td>
    <td><%= link_to 'Show', cheat %></td>
    <td><%= link_to 'Edit', edit_cheat_path(cheat) %></td>
    <td><%= link_to 'Destroy', cheat, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New cheat', new_cheat_path %>